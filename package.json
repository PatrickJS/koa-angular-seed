{
  "name": "koa-angular-seed",
  "version": "0.1.1",
  "description": "An Angular.js seed for Koa apps with gulp, stylus, and browserify",
  "main": "server.js",
  "scripts": {
    "prepublish": "node npm-scripts prepublish",
    "postpublish": "node npm-scripts postpublish",
    "preinstall": "node npm-scripts preinstall",
    "postinstall": "node npm-scripts postinstall",
    "preuninstall": "node npm-scripts preuninstall",
    "postuninstall": "node npm-scripts postuninstall",
    "preupdate": "node npm-scripts preupdate",
    "postupdate": "node npm-scripts postupdate",
    "test": "karma start test/karma.conf.js",
    "start": "nodemon --harmony server.js"
  },
  "author": [
    {
      "name": "Patrick Stapleton",
      "email": "github@gdi2290.com",
      "url": "www.gdi2290.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/gdi2290/koa-angular-seed.git"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ],
  "keywords": [
    "koa",
    "angular",
    "seed",
    "angularjs",
    "koajs"
  ],
  "dependencies": {
    "koa": "latest",
    "koa-render": "latest",
    "koa-route": "latest",
    "koa-logger": "latest",
    "jade": "latest",
    "koa-static": "~1.4.2"
  },
  "devDependencies": {
    "tiny-lr": "0.0.5",
    "gulp": "latest",
    "gulp-stylus": "latest",
    "gulp-livereload": "~0.2.0",
    "gulp-concat": "~2.1.7",
    "gulp-browserify": "~0.2.4",
    "gulp-uglify": "git://github.com/jonathanepollack/gulp-uglify.git#master",
    "gulp-clean": "~0.2.2",
    "gulp-imagemin": "~0.1.4",
    "gulp-ngmin": "~0.1.1"
  },
  "engines": {
    "node": "0.11.x"
  }
}
